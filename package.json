{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "Simple boilerplate with React and Parcel",
  "main": "index.js",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-minify": "^0.5.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "recompose": "^0.30.0",
    "sass": "^1.15.2",
    "sinon": "^7.2.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "parcel -p 3000 src/client/html/index.html -d tmp/",
    "build:client": "NODE_ENV=production parcel build src/client/html/index.html -d dist/static/ --public-url /static",
    "build:server": "NODE_ENV=production babel src/server/index.js -d dist/",
    "build": "npm run build:server && npm run build:client",
    "lint": "eslint src/",
    "test": "mocha 'test/**/*_test.{js,jsx}'",
    "clean": "rm -rf dist/ tmp/"
  },
  "keywords": [],
  "author": "Karol Heczko",
  "license": "MIT"
}
